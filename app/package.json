  "scripts": {
    "dev": "next dev -p 3001",
    "dev:debug": "NODE_OPTIONS='--inspect=9300' next dev -p 3001 & npm run scss:class-types:watch",
    "build": "next build",
    "start": "next start",
    "scss:class-types": "tsm 'src/**/**/*.scss' --ignore 'src/theme/*.scss' --exportType default --nameFormat none --implementation sass src --logLevel silent",
    "scss:class-types:watch": "npm run scss:class-types -- --watch",
    "plop": "plop",
    "api:codegen:watch": "graphql-codegen --watch 'src/pages/api/**/resolvers/*.ts' --watch 'src/pages/api/**/schema.graphql'",
    "api:codegen": "npm run api:codegen:dry & npm run api:codegen:dry:strapi",
    "api:codegen:dry": "graphql-codegen",
    "api:codegen:dry:strapi": "graphql-codegen --config codegen.strapi.yml",
    "postinstall": "npm run api:codegen"
  },
